import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		int number;
		System.out.println("Enter a number");
		
		Scanner sc = new Scanner(System.in);
		number = sc.nextInt();
		
		if(number%2==0)
		{
		    System.out.println("Number is even.");
		}
		
		else
		{
		    System.out.println("Number is odd.");
		}
	}
}
